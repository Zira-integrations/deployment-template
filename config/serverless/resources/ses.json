{
    "Resources": {
        "ReceiptRuleSet": {
            "Type": "AWS::SES::ReceiptRuleSet",
            "Properties": {
                "RuleSetName": "ruleSet"
            }
        },
        "ReceiptRule": {
            "Type": "AWS::SES::ReceiptRule",
            "Properties": {
                "RuleSetName": "ruleSet",
                "Rule": {
                    "Name": "ruleForTest",
                    "Enabled": true,
                    "ScanEnabled": true,
                    "Recipients": [
                        "test@int.zira.us"
                    ],
                    "Actions": [
                        {
                            "S3Action": {
                                "BucketName": "integrations-data-zira-${self:provider.stage}",
                                "ObjectKeyPrefix": "${self:context.testSite.prefix}"
                            }
                        }
                    ]
                }
                
            }
        },
        "ReceiptRule2": {
            "Type": "AWS::SES::ReceiptRule",
            "Properties": {
                "RuleSetName": "ruleSet",
                "Rule": {
                    "Name": "ruleForTest2",
                    "Enabled": true,
                    "ScanEnabled": true,
                    "Recipients": [
                        "test2@int.zira.us"
                    ],
                    "Actions": [
                        {
                            "S3Action": {
                                "BucketName": "integrations-data-zira-${self:provider.stage}",
                                "ObjectKeyPrefix": "${self:context.testSite2.prefix}"
                            }
                        }
                    ]
                }
                
            }
        },
        "DomainIdentityResource": {
            "Type": "AWS::SES::EmailIdentity",
            "Properties": {
                "EmailIdentity": "int.zira.us"
            }
        }

    }
}